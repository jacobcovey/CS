#include <iostream>
#include "LinkedListInterface.h"
#include "LinkedList.h"

using namespace std;

int main() {
	LinkedList<int> list;


	list.insertHead(1);
	list.insertHead(2);
	list.insertHead(3);
	list.insertTail(4);
	list.insertTail(2);
	list.insertAfter(6,2);
	list.insertAfter(3,1);
	cout << "Value: " <<list.at(3) << endl;

	return 0;
}
